<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping
DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-
3.0.dtd">
<hibernate-mapping package="com.example.model">
	<class name="Residencial" table="RESIDENCIAL">
		<id name="id" column="ID_RESIDENCIAL">
			<generator class="native" />
		</id>
		<property name="nombre" column="NOMBRE" />
		<property name="numeroViviendas" column="NVIVIENDAS" />
		<property name="distrito" column="DISTRITO" />
	</class>
</hibernate-mapping>
